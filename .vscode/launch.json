    "version": "0.2.0",
    "configurations": [
        {
            "name": "Angular ng serve",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:4200/",
            "cwd": "${workspaceFolder}/filemanager",
            "preLaunchTask": "Angular: start",
            "webRoot": "${workspaceFolder}/filemanager"
        },
        {
            "type": "node-terminal",
            "name": "Filemanager start",
            "request": "launch",
            "command": "npm start",
            "cwd": "${workspaceFolder}/node-filesystem-provider"
        },
    ]
}